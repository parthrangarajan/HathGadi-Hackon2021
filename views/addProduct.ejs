            <div class="container">
                <div class="p-4 m-4">
                    <div class= "col-lg-5 mx-auto border rounded pt-4 m-4 p-4  mt-4 mb-4 m-4 p-3" style="background-color: white;">
                        <h3 class="alert alert-light border rounded">Enter new product details :</h3>
                        <form action="/new" method="POST" autocomplete="off">
                          <div class="form-group a-4 m-4">
                                <label for="ProductName">Product Name :</label>
                                <input type="text" name="ProductName" placeholder="Enter product name" autocomplete="off" required/>
                            </div>
                            <div class="form-group a-4 m-4">
                                <label for="ProductType">Product Type :</label>
                                <input type="text" name="ProductType" placeholder="Enter category for product" autocomplete="off" required/>
                            </div>
                            <div class="form-group a-4 m-4">
                                <label for="ProductQuantity">Product Quantity :</label>
                                <input type="number" name="ProductQuantityIn" placeholder="What is the product kg/dozen/bunch" autocomplete="off" required>
                            </div>
                            <div class="form-group a-4 m-4">
                                <label for="ProductPrice">Product Price :</label>
                                <input type="number" name="ProductPer" placeholder="Enter your product price here" autocomplete="off" required>
                            </div>
                            <div class="form-group a-4 m-4">
                                <label for="ProductPer">Product Per :</label>
                                <input type="text" name="ProductPer" placeholder="Enter your product/quantity here" autocomplete="off" required>
                            </div>
                            <div class="form-group a-4 m-4">
                                <label for="ProductDesc">Product Description :</label>
                                <input type="text" name="ProductDesc" placeholder="Enter your product description here" autocomplete="off" required>
                            </div>
                            <br>
                            <br>


                            <div class="form-group ">
                                <input class="btn btn-sm btn-info" type="submit" value="Submit new product details">
                            </div>

                        </form>
                    </div>
            </div>
